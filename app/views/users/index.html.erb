<section class="users-section">
  <ul class="users-list">
    <% @users.each do |user| %>
      <li>
        Name: <%= user.name %>
        <span class="profile-link">
          <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
        </span>
        <span><%= link_to 'add friend', new_user_session_path, class: 'btn btn-success'%></span>
      </li>
    <% end %>
  </ul>
</section>
